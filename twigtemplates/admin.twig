<div class="wrapper admin">
	<h2>Заказы</h2>
	<table>
	<tr>
		<td>№</td>
		<td>Имя</td>
		<td>Телефон</td>
		<td>ID товара</td>
		<td>Количество товара</td>
		<td>Статус</td>
	</tr>

	{% for item in admin %}

		<tr>
			<td>{{ item.number }}</td>
			<td>{{ item.name }}</td>
			<td>{{ item.phone }}</td>
			<td>{{ item.id_product }}</td>
			<td>{{ item.quantity }}</td>
			<td>{{ item.status }}</td>
		</tr>

	{% endfor %}

	</table>
</div>










