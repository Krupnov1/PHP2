<div class="catalogs">

	{% for item in catalog %}

		<section class="product">
			<h3>{{item.name}}</h3>
			<h4>({{item.likes}})</h4>
			<a href="/product/card/?id={{item.id}}"><img src="/img/{{item.image_file}}" alt="{{item.image_alt}}" width='320' height='341'></a>

		</section>

	{% endfor %}

</div>
<a href="/product/catalog/?page={{page}}">Далее</a>